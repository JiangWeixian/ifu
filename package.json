{
  "name": "ifu",
  "version": "",
  "scripts": {
    "lint:fix": "eslint . --fix",
    "build": "pnpm build -r",
    "ci:publish": "pnpm run build && pnpx changeset publish",
    "ci:version": "pnpx changeset version",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.3.2",
    "@changesets/cli": "^2.16.0",
    "@types/node": "^17.0.5",
    "eslint": "^7.31.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "tsc-progress": "^1.0.1",
    "ttypescript": "^1.5.13",
    "typescript-transform-extensions": "^1.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
