  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "pnpm run build && ava",
    "build": "tsc",
    "postinstall": "ifu -c || alfy-init",
    "preuninstall": "alfy-cleanup",
    "ci:publish": "pnpm run build && pnpx changeset publish",
    "ci:version": "pnpx changeset version",
    "ci:snapshot": "pnpx changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpx changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "alfy": "^0.10.0",
    "dayjs": "^1.8.36",
    "lodash.isnan": "^3.0.2",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@aiou/eslint-config": "latest",
    "@changesets/cli": "^2.16.0",
    "@ifu/cli": "^0.0.3",
    "@types/lodash.isnan": "^3.0.6",
    "alfy-test": "^0.4.0",
    "ava": "^3.12.1",
    "eslint": "^7.x",
    "esm": "^3.2.25",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.0",
    "prettier": "^2.3.2",
    "typescript": "^4.0.2"
  },
  "ava": {
    "require": [
      "esm"
    ]
