    "rollup-plugin-condition-exports": "^1.4.1",
    "rollup-plugin-multi-input": "1.3.1",
    "rollup-plugin-typescript2": "0.30.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "types": "dist/index.d.ts"
