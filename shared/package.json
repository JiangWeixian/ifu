      "import": "./dist/shortcuts.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "lunar": [
        "dist/lunar.d.ts"
      ],
      "popcron": [
        "dist/popcron.d.ts"
      ],
      "shortcuts": [
        "dist/shortcuts.d.ts"
      ]
